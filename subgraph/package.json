{
  "name": "ens-subgraph",
  "scripts": {
    "codegen": "npx graph codegen --output-dir src/types/ subgraph.yaml",
    "build": "npx graph build subgraph.yaml",
    "build-ipfs": "npx graph build --ipfs /ip4/127.0.0.1/tcp/5001 subgraph.yaml",
    "build-wast": "npx graph build -t wast subgraph.yaml",
    "deploy": "npx graph deploy --ipfs /ip4/35.239.68.19/tcp/5001 --node http://104.198.242.70:8020/ -n ens subgraph.yaml --api-key <KEY>"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.3.1",
    "@graphprotocol/graph-ts": "^0.0.1"
  },
  "dependencies": {
    "eth-ens-namehash": "^2.0.8",
    "web3": "^1.0.0-beta.36"
  }
}
